package VU;
import javax.swing.*;
public class PersonInfo {

	String name;
	String address;
	String number;
	
	public PersonInfo(String name,String address,String number) {
		this.name=name;
		this.address=address;
		this.number=number;
	}
	public void showInfo(){
		 JOptionPane.showMessageDialog(null,"The number is"+number+" Address: "+address+" number: "+number);
	}
	
	public static void main(String[] args) {
		
		String s = JOptionPane.showInputDialog("Enter an integer");
		   

	}

}
