public class MyClass extends Thread {

	public void run() {
		for (int i = 0; i < 11; i++) {
			System.out.println("value:" + i);
		}
	}
}

class Demo {

	public static void main(String args[]) {

		MyClass myClass= new MyClass();
		myClass.start();
		MyClass myClass1= new MyClass();
		myClass.start();
	}
}
