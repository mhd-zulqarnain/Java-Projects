public class Box {
	public double width;
	public double height;
	public double depth;

	// constructor

	public Box() {
	}
	
	public Box (Box Obj)
	{
		this.width=Obj.width;
		this.height=Obj.height;
		this.depth=Obj.depth;
	}
	
	public Box(double value)
	{
		width=height=depth=value*3;
	}
	
	public void show(){
		System.out.println("w h d:"+width+height+depth);
	}
}

class B extendsS

