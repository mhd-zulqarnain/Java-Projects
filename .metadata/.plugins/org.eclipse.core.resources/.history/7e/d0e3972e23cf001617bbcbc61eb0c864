
import java.util.*;
public class LlistDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		LinkedList<String> ll= new LinkedList<String>();
		
		ll.add("C");
		ll.add("G");
		ll.add("H");
		ll.add("I");
		ll.addLast("Z");
		ll.addFirst("A");
		
		System.out.println("Orignal list "+ll);
		
	}

}
